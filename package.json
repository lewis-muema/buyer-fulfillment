{
  "name": "buyer-fulfillment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env DOCKER_ENV=\"development\" vue-cli-service serve",
    "build": "cross-env DOCKER_ENV=\"production\" vue-cli-service build",
    "beta": "cross-env DOCKER_ENV=\"beta\" vue-cli-service build",
    "staging": "cross-env DOCKER_ENV=\"staging\"  vue-cli-service build",
    "lint": "cross-env DOCKER_ENV=\"development\" vue-cli-service lint --fix",
    "test:unit": "nyc cross-env DOCKER_ENV=\"development\" vue-cli-service test:unit",
    "showcoverage": "nyc report --reporter html --reporter text -t coverage --report-dir coverage/summary"
  },
  "dependencies": {
    "@element-plus/icons": "^0.0.11",
    "@element-plus/icons-vue": "^2.0.9",
    "@fawmi/vue-google-maps": "^0.9.79",
    "@firebase/messaging": "^0.9.16",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@popperjs/core": "^2.11.6",
    "@sendyit/pay": "^0.1.24",
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.1",
    "bootstrap-icons": "^1.9.1",
    "core-js": "^3.8.3",
    "element-plus": "^2.2.17",
    "firebase": "^9.10.0",
    "moment": "^2.29.4",
    "vue": "^3.2.13",
    "vue-i18n": "^9.3.0-beta.6",
    "vue-material-design-icons": "^5.1.2",
    "vue-router": "^4.0.3",
    "vue-tel-input": "^6.0.5",
    "vuejs3-datepicker": "^1.0.12",
    "vuex": "^4.0.0",
    "webpack": "^5.74.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-unit-mocha": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-vue": "^8.0.3",
    "eslint-plugin-vuejs-accessibility": "^1.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  }
}
